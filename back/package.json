{
  "name": "back",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:generate": "yarn typeorm migration:generate -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "console": "ts-node -r tsconfig-paths/register src/console.ts",
    "console:dev": "NODE_ENV=dev yarn console"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.2",
    "@nestjs/core": "^7.5.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@uppy/companion": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "commander": "^7.0.0",
    "date-fns": "^2.17.0",
    "express-session": "^1.17.2",
    "faker": "^5.2.0",
    "futoin-hkdf": "^1.3.2",
    "getstream": "^7.2.1",
    "googleapis": "39",
    "jose": "2.0.3",
    "nestjs-console": "^4.0.0",
    "nestjs-i18n": "^8.0.11",
    "passport": "^0.4.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sharp": "^0.29.3",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.30",
    "url-slug": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.5.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "~(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
