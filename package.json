{
  "name": "pokemon-generator",
  "version": "1.0.0",
  "scripts": {
    "i": "yarn install",
    "install": "yarn client:install && yarn server:install",
    "client:install": "cd client && yarn",
    "server:install": "cd back && yarn",
    "server:dev": "cd back && yarn start",
    "server:build": "cd back && yarn build",
    "server:start": "cd back && yarn start",
    "client:dev": "cd client && yarn dev",
    "client:build": "cd client && yarn build",
    "client:start": "cd client && yarn start"
  }
}
